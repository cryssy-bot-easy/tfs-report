<?xml version="1.0" encoding="UTF-8"?>
<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="YTD_Transaction_Count_Import_and_Export" language="groovy" pageWidth="612" pageHeight="792" whenNoDataType="AllSectionsNoDetail" columnWidth="540" leftMargin="36" rightMargin="36" topMargin="36" bottomMargin="36" isSummaryWithPageHeaderAndFooter="true">
	<property name="ireport.zoom" value="1.5"/>
	<property name="ireport.x" value="0"/>
	<property name="ireport.y" value="0"/>
	<parameter name="reportId" class="java.lang.String"/>
	<parameter name="branchUnitCode" class="java.lang.String"/>
	<parameter name="branchUnitName" class="java.lang.String">
		<defaultValueExpression><![CDATA[""]]></defaultValueExpression>
	</parameter>
	<parameter name="currentMonth" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="currentYear" class="java.lang.String">
		<parameterDescription><![CDATA[]]></parameterDescription>
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="reportDate" class="java.lang.String"/>
	<queryString>
		<![CDATA[--YTD TRANSACTION COUNT
select
	-- CLS
	(SELECT COUNT(DISTINCT AFCIF_NO) AS TOTAL
	 FROM REF_LNAPPF

	 WHERE AFFCDE IN ('FBE','FBF','FDB','FDT','FD1','FD2','FD3','FD4','FD5','FE1','FE2','FFB','FFT','FF1','FF2','FF3','FF5','FTF','F1E','F1F','F2E','F2F','F3F','F3G','F3I',
					  'F3K','F3S','F3U','F3Y','F3Z','F5F','LLC','FCN'))as cls,


	-- FOR NON CLS
	(SELECT SUM(ALL1.COUNT1) AS TOTAL

	 FROM (SELECT COUNT(DISTINCT cb_cd) AS COUNT1
		   FROM REF_TFCUSTMR

		   UNION ALL

		   SELECT COUNT(DISTINCT AABREFCODE) AS COUNT1
		   FROM REFPAS5CLIENT

		   WHERE CIFNO IS NULL

		  ) AS ALL1

	) as noncls,


	-- FOR IMPORTS
	(SELECT count(distinct DOCUMENTNUMBER)
	 FROM LETTEROFCREDIT

	 WHERE DOCUMENTTYPE = 'FOREIGN'
	 AND TYPE = 'REGULAR'
	 and (1<=$P{currentMonth} and year(PROCESSDATE)=$P{currentYear}))as fxregulardistinct,


	(SELECT count(distinct DOCUMENTNUMBER)
	 FROM LETTEROFCREDIT

	 WHERE DOCUMENTTYPE = 'FOREIGN'
	 AND TYPE = 'CASH'
	 and (1<=$P{currentMonth} and year(PROCESSDATE)=$P{currentYear}))as fxcashdistinct,


	(SELECT count(distinct DOCUMENTNUMBER)
	 FROM LETTEROFCREDIT

	 WHERE DOCUMENTTYPE = 'FOREIGN'
	 AND TYPE = 'STANDBY'
	 and (1<=$P{currentMonth} and year(PROCESSDATE)=$P{currentYear}))as fxstandbydistinct,


	(SELECT count(distinct DOCUMENTNUMBER)
	 FROM LETTEROFCREDIT

	 WHERE DOCUMENTTYPE = 'DOMESTIC'
	 AND TYPE = 'REGULAR'
	 and (1<=$P{currentMonth} and year(PROCESSDATE)=$P{currentYear}))as dmregulardistinct,


	(SELECT count(distinct DOCUMENTNUMBER)
	 FROM LETTEROFCREDIT

	 WHERE DOCUMENTTYPE = 'DOMESTIC'
	 AND TYPE = 'CASH'
	 and (1<=$P{currentMonth} and year(PROCESSDATE)=$P{currentYear}))as dmcashdistinct,


	(SELECT count(distinct DOCUMENTNUMBER)
	 FROM LETTEROFCREDIT

	 WHERE DOCUMENTTYPE = 'DOMESTIC'
	 AND TYPE = 'STANDBY'
	 and (1<=$P{currentMonth} and year(PROCESSDATE)=$P{currentYear}))as dmstandbydistinct,


	(SELECT count(distinct DOCUMENTNUMBER)
	 FROM DOCUMENTAGAINSTACCEPTANCE
	 WHERE (1<=$P{currentMonth} and year(PROCESSDATE)=$P{currentYear}))as fxdadistinct,


	(SELECT count(distinct DOCUMENTNUMBER)
	 FROM DOCUMENTAGAINSTPAYMENT

	 WHERE DOCUMENTTYPE = 'FOREIGN'
	 and (1<=$P{currentMonth} and year(PROCESSDATE)=$P{currentYear}))as fxdpdistinct,


	(SELECT count(distinct DOCUMENTNUMBER)
	 FROM DIRECTREMITTANCE
	 WHERE (1<=$P{currentMonth} and year(PROCESSDATE)=$P{currentYear}))as fxdrdistinct,


	(SELECT count(distinct DOCUMENTNUMBER)
	 FROM OPENACCOUNT
	 WHERE (1<=$P{currentMonth} and year(PROCESSDATE)=$P{currentYear}))as fxoadistinct,


	(SELECT count(distinct DOCUMENTNUMBER)
	 FROM DOCUMENTAGAINSTPAYMENT

	 WHERE DOCUMENTTYPE = 'DOMESTIC'
	 and (1<=$P{currentMonth} and year(PROCESSDATE)=$P{currentYear}))as dmnonlcdistinct,


	(SELECT count(amount)
	 FROM CDTPAYMENTREQUEST
	 WHERE (1<=$P{currentMonth} and year(DATE_SENT)=$P{currentYear}))as otherscdtdistinct,


	(select count(distinct tradeproductnumber) from tradeservice ts
		where documentclass='IMPORT_ADVANCE'
		and (status='APPROVED' or status='POSTED' or status='POST_APPROVED')
		and (1<=$P{currentMonth} and year(modifieddate)=$P{currentYear}))as othersimportadvancedistinct,

	-- FOR EXPORTS
	(select count(servicetype) from tradeservice ts
		where documentclass='EXPORT_ADVISING'
		and (status='APPROVED' or status='POSTED' or status='POST_APPROVED')
		and (1<=$P{currentMonth} and year(modifieddate)=$P{currentYear}))as exportadvisingdistinct,

	(select count(servicetype) from tradeservice ts
		where servicetype='NEGOTIATION'
		and documentType='DOMESTIC' and documentclass='BC'
		and (status='APPROVED' or status='POSTED' or status='POST_APPROVED')
		and (1<=$P{currentMonth} and year(modifieddate)=$P{currentYear}))
		as dbcnegotiationdistinct,

	(select count(servicetype) from tradeservice ts
		where servicetype='NEGOTIATION'
		and documentType='DOMESTIC' and documentclass='BP'
		and (status='APPROVED' or status='POSTED' or status='POST_APPROVED')
		and (1<=$P{currentMonth} and year(modifieddate)=$P{currentYear}))
		as dbpnegotiationdistinct,

	(select count(servicetype) from tradeservice ts
		where servicetype='NEGOTIATION'
		and documentType='FOREIGN' and documentclass='BC'
		and (status='APPROVED' or status='POSTED' or status='POST_APPROVED')
		and (1<=$P{currentMonth} and year(modifieddate)=$P{currentYear}))
		as ebcnegotiationdistinct,

	(select count(servicetype) from tradeservice ts
		where servicetype='NEGOTIATION'
		and documentType='FOREIGN' and documentclass='BP'
		and (status='APPROVED' or status='POSTED' or status='POST_APPROVED')
		and (1<=$P{currentMonth} and year(modifieddate)=$P{currentYear}))
		as ebpnegotiationdistinct,

	(select count(servicetype) from tradeservice ts
		where servicetype='SETTLEMENT'
		and (documentclass='BC' or documentclass='BP')
		and (status='APPROVED' or status='POSTED' or status='POST_APPROVED')
		and (1<=$P{currentMonth} and year(modifieddate)=$P{currentYear}))
		as exportbillssettlementdistinct,

	(select count(servicetype) from tradeservice ts
		where documentclass='EXPORT_ADVANCE'
		and (status='APPROVED' or status='POSTED' or status='POST_APPROVED')
		and (1<=$P{currentMonth} and year(modifieddate)=$P{currentYear}))
		as othersexportadvancedistinct,

	-- FOR FOREIGN REGULAR
	(select count(servicetype) from tradeservice ts
		where documentclass='LC' and documenttype='FOREIGN'
		and documentsubtype1='REGULAR' and servicetype='OPENING'
		and (status='APPROVED' or status='POSTED' or status='POST_APPROVED')
		and (1<=$P{currentMonth} and year(modifieddate)=$P{currentYear}))as fxregularopening,

	-- FOR FX ADJUSTMENT REG-CASH
	(select count(servicetype) from tradeservice ts
		INNER JOIN TRADEPRODUCT AS TP
		ON TP.DOCUMENTNUMBER = TS.TRADEPRODUCTNUMBER
		JOIN LETTEROFCREDIT AS LC
		ON LC.DOCUMENTNUMBER = TP.DOCUMENTNUMBER
		where TS.documentclass='LC' and TS.documenttype='FOREIGN'
		and TS.documentsubtype1='REGULAR' and TS.servicetype='ADJUSTMENT'
		AND LC.CASHFLAG = 1
		and (TS.status='APPROVED' or TS.status='POSTED' or TS.status='POST_APPROVED')
		and (1<=$P{currentMonth} and year(TS.modifieddate)=$P{currentYear}))as fxregcashadjustment,

	-- FOR FX ADJUSTMENT REG
	(select count(servicetype) from tradeservice ts
		INNER JOIN TRADEPRODUCT AS TP
		ON TP.DOCUMENTNUMBER = TS.TRADEPRODUCTNUMBER
		JOIN LETTEROFCREDIT AS LC
		ON LC.DOCUMENTNUMBER = TP.DOCUMENTNUMBER
		where TS.documentclass='LC' and TS.documenttype='FOREIGN'
		and TS.documentsubtype1='REGULAR' and TS.servicetype='ADJUSTMENT'
		AND LC.CASHFLAG IS NULL
		and (TS.status='APPROVED' or TS.status='POSTED' or TS.status='POST_APPROVED')
		and (1<=$P{currentMonth} and year(TS.modifieddate)=$P{currentYear}))as fxregadjustment,

	(select count(servicetype) from tradeservice ts
		where documentclass='LC' and documenttype='FOREIGN'
		and documentsubtype1='REGULAR' and servicetype='CANCELLATION'
		and (status='APPROVED' or status='POSTED' or status='POST_APPROVED')
		and (1<=$P{currentMonth} and year(modifieddate)=$P{currentYear}))as fxregularcancellation,

	(select count(servicetype) from tradeservice ts
		where documentclass='LC' and documenttype='FOREIGN'
		and documentsubtype1='REGULAR' and servicetype='AMENDMENT'
		and (status='APPROVED' or status='POSTED' or status='POST_APPROVED')
		and (1<=$P{currentMonth} and year(modifieddate)=$P{currentYear}))as fxregularamendment,

	(select count(servicetype) from tradeservice ts
		where documentclass='INDEMNITY' and documenttype='FOREIGN'
		and documentsubtype1='REGULAR' and servicetype='ISSUANCE'
		and (status='APPROVED' or status='POSTED' or status='POST_APPROVED')
		and (1<=$P{currentMonth} and year(modifieddate)=$P{currentYear}))as fxregularbgissuance,

	(select count(servicetype) from tradeservice ts
		where documentclass='INDEMNITY' and documenttype='FOREIGN'
		and documentsubtype1='REGULAR' and servicetype='CANCELLATION'
		and (status='APPROVED' or status='POSTED' or status='POST_APPROVED')
		and (1<=$P{currentMonth} and year(modifieddate)=$P{currentYear}))as fxregularbgcancellation,

	(select count(servicetype) from tradeservice ts
		where documentclass='LC' and documenttype='FOREIGN'
		and documentsubtype1='REGULAR' and servicetype='NEGOTIATION'
		and (status='APPROVED' or status='POSTED' or status='POST_APPROVED')
		and (1<=$P{currentMonth} and year(modifieddate)=$P{currentYear}))as fxregularnegotiation,

	(select count(servicetype) from tradeservice ts
		where documentclass='LC' and documenttype='FOREIGN'
		and documentsubtype1='REGULAR' and servicetype='UA_LOAN_MATURITY_ADJUSTMENT'
		and (status='APPROVED' or status='POSTED' or status='POST_APPROVED')
		and (1<=$P{currentMonth} and year(modifieddate)=$P{currentYear}))
		as fxregularmaturityadjustment,

	(select count(servicetype) from tradeservice ts
		where documentclass='LC' and documenttype='FOREIGN'
		and documentsubtype1='REGULAR' and servicetype='UA_LOAN_SETTLEMENT'
		and (status='APPROVED' or status='POSTED' or status='POST_APPROVED')
		and (1<=$P{currentMonth} and year(modifieddate)=$P{currentYear}))as fxregularsettlement,

	-- FOR FOREIGN CASH
	(select count(servicetype) from tradeservice ts
		where documentclass='LC' and documenttype='FOREIGN'
		and documentsubtype1='CASH' and servicetype='OPENING'
		and (status='APPROVED' or status='POSTED' or status='POST_APPROVED')
		and (1<=$P{currentMonth} and year(modifieddate)=$P{currentYear}))as fxcashopening,

	(select count(servicetype) from tradeservice ts
		where documentclass='LC' and documenttype='FOREIGN'
		and documentsubtype1='CASH' and servicetype='ADJUSTMENT'
		and (status='APPROVED' or status='POSTED' or status='POST_APPROVED')
		and (1<=$P{currentMonth} and year(modifieddate)=$P{currentYear}))as fxcashadjustment,

	(select count(servicetype) from tradeservice ts
		where documentclass='LC' and documenttype='FOREIGN'
		and documentsubtype1='CASH' and servicetype='CANCELLATION'
		and (status='APPROVED' or status='POSTED' or status='POST_APPROVED')
		and (1<=$P{currentMonth} and year(modifieddate)=$P{currentYear}))as fxcashcancellation,

	(select count(servicetype) from tradeservice ts
		where documentclass='LC' and documenttype='FOREIGN'
		and documentsubtype1='CASH' and servicetype='AMENDMENT'
		and (status='APPROVED' or status='POSTED' or status='POST_APPROVED')
		and (1<=$P{currentMonth} and year(modifieddate)=$P{currentYear}))as fxcashamendment,

	(select count(servicetype) from tradeservice ts
		where documentclass='LC' and documenttype='FOREIGN'
		and documentsubtype1='CASH' and servicetype='NEGOTIATION'
		and (status='APPROVED' or status='POSTED' or status='POST_APPROVED')
		and (1<=$P{currentMonth} and year(modifieddate)=$P{currentYear}))as fxcashnegotiation,

	-- FOR FOREIGN STANDBY
	(select count(servicetype) from tradeservice ts
		where documentclass='LC' and documenttype='FOREIGN'
		and documentsubtype1='STANDBY' and servicetype='OPENING'
		and (status='APPROVED' or status='POSTED' or status='POST_APPROVED')
		and (1<=$P{currentMonth} and year(modifieddate)=$P{currentYear}))as fxstandbyopening,

	(select count(servicetype) from tradeservice ts
		where documentclass='LC' and documenttype='FOREIGN'
		and documentsubtype1='STANDBY' and servicetype='ADJUSTMENT'
		and (status='APPROVED' or status='POSTED' or status='POST_APPROVED')
		and (1<=$P{currentMonth} and year(modifieddate)=$P{currentYear}))as fxstandbyadjustment,

	(select count(servicetype) from tradeservice ts
		where documentclass='LC' and documenttype='FOREIGN'
		and documentsubtype1='STANDBY' and servicetype='CANCELLATION'
		and (status='APPROVED' or status='POSTED' or status='POST_APPROVED')
		and (1<=$P{currentMonth} and year(modifieddate)=$P{currentYear}))as fxstandbycancellation,

	(select count(servicetype) from tradeservice ts
		where documentclass='LC' and documenttype='FOREIGN'
		and documentsubtype1='STANDBY' and servicetype='AMENDMENT'
		and (status='APPROVED' or status='POSTED' or status='POST_APPROVED')
		and (1<=$P{currentMonth} and year(modifieddate)=$P{currentYear}))as fxstandbyamendment,

	(select count(servicetype) from tradeservice ts
		where documentclass='LC' and documenttype='FOREIGN'
		and documentsubtype1='STANDBY' and servicetype='NEGOTIATION'
		and (status='APPROVED' or status='POSTED' or status='POST_APPROVED')
		and (1<=$P{currentMonth} and year(modifieddate)=$P{currentYear}))as fxstandbynegotiation,

	-- FOR DOMESTIC REGULAR
	(select count(servicetype) from tradeservice ts
		where documentclass='LC' and documenttype='DOMESTIC'
		and documentsubtype1='REGULAR' and servicetype='OPENING'
		and (status='APPROVED' or status='POSTED' or status='POST_APPROVED')
		and (1<=$P{currentMonth} and year(modifieddate)=$P{currentYear}))as dmregularopening,

	-- FOR DM ADJUSTMENT REG-CASH
	(select count(servicetype) from tradeservice ts
		INNER JOIN TRADEPRODUCT AS TP
		ON TP.DOCUMENTNUMBER = TS.TRADEPRODUCTNUMBER
		JOIN LETTEROFCREDIT AS LC
		ON LC.DOCUMENTNUMBER = TP.DOCUMENTNUMBER
		where TS.documentclass='LC' and TS.documenttype='DOMESTIC'
		and TS.documentsubtype1='REGULAR' and TS.servicetype='ADJUSTMENT'
		AND LC.CASHFLAG = 1
		and (TS.status='APPROVED' or TS.status='POSTED' or TS.status='POST_APPROVED')
		and (1<=$P{currentMonth} and year(TS.modifieddate)=$P{currentYear}))as dmregcashadjustment,

	-- FOR DM ADJUSTMENT REG
	(select count(servicetype) from tradeservice ts
		INNER JOIN TRADEPRODUCT AS TP
		ON TP.DOCUMENTNUMBER = TS.TRADEPRODUCTNUMBER
		JOIN LETTEROFCREDIT AS LC
		ON LC.DOCUMENTNUMBER = TP.DOCUMENTNUMBER
		where TS.documentclass='LC' and TS.documenttype='DOMESTIC'
		and TS.documentsubtype1='REGULAR' and TS.servicetype='ADJUSTMENT'
		AND LC.CASHFLAG IS NULL
		and (TS.status='APPROVED' or TS.status='POSTED' or TS.status='POST_APPROVED')
		and (1<=$P{currentMonth} and year(TS.modifieddate)=$P{currentYear}))as dmregadjustment,

	(select count(servicetype) from tradeservice ts
		where documentclass='LC' and documenttype='DOMESTIC'
		and documentsubtype1='REGULAR' and servicetype='CANCELLATION'
		and (status='APPROVED' or status='POSTED' or status='POST_APPROVED')
		and (1<=$P{currentMonth} and year(modifieddate)=$P{currentYear}))as dmregularcancellation,

	(select count(servicetype) from tradeservice ts
		where documentclass='LC' and documenttype='DOMESTIC'
		and documentsubtype1='REGULAR' and servicetype='AMENDMENT'
		and (status='APPROVED' or status='POSTED' or status='POST_APPROVED')
		and (1<=$P{currentMonth} and year(modifieddate)=$P{currentYear}))as dmregularamendment,

	(select count(servicetype) from tradeservice ts
		where documentclass='LC' and documenttype='DOMESTIC'
		and documentsubtype1='REGULAR' and servicetype='NEGOTIATION'
		and (status='APPROVED' or status='POSTED' or status='POST_APPROVED')
		and (1<=$P{currentMonth} and year(modifieddate)=$P{currentYear}))as dmregularnegotiation,

	(select count(servicetype) from tradeservice ts
		where documentclass='LC' and documenttype='DOMESTIC'
		and documentsubtype1='REGULAR' and servicetype='UA_LOAN_MATURITY_ADJUSTMENT'
		and (status='APPROVED' or status='POSTED' or status='POST_APPROVED')
		and (1<=$P{currentMonth} and year(modifieddate)=$P{currentYear}))
		as dmregularmaturityadjustment,

	(select count(servicetype) from tradeservice ts
		where documentclass='LC' and documenttype='DOMESTIC'
		and documentsubtype1='REGULAR' and servicetype='UA_LOAN_SETTLEMENT'
		and (status='APPROVED' or status='POSTED' or status='POST_APPROVED')
		and (1<=$P{currentMonth} and year(modifieddate)=$P{currentYear}))as dmregularsettlement,

	-- FOR DOMESTIC CASH
	(select count(servicetype) from tradeservice ts
		where documentclass='LC' and documenttype='DOMESTIC'
		and documentsubtype1='CASH' and servicetype='OPENING'
		and (status='APPROVED' or status='POSTED' or status='POST_APPROVED')
		and (1<=$P{currentMonth} and year(modifieddate)=$P{currentYear}))as dmcashopening,

	(select count(servicetype) from tradeservice ts
		where documentclass='LC' and documenttype='DOMESTIC'
		and documentsubtype1='CASH' and servicetype='ADJUSTMENT'
		and (status='APPROVED' or status='POSTED' or status='POST_APPROVED')
		and (1<=$P{currentMonth} and year(modifieddate)=$P{currentYear}))as dmcashadjustment,

	(select count(servicetype) from tradeservice ts
		where documentclass='LC' and documenttype='DOMESTIC'
		and documentsubtype1='CASH' and servicetype='CANCELLATION'
		and (status='APPROVED' or status='POSTED' or status='POST_APPROVED')
		and (1<=$P{currentMonth} and year(modifieddate)=$P{currentYear}))as dmcashcancellation,

	(select count(servicetype) from tradeservice ts
		where documentclass='LC' and documenttype='DOMESTIC'
		and documentsubtype1='CASH' and servicetype='AMENDMENT'
		and (status='APPROVED' or status='POSTED' or status='POST_APPROVED')
		and (1<=$P{currentMonth} and year(modifieddate)=$P{currentYear}))as dmcashamendment,

	(select count(servicetype) from tradeservice ts
		where documentclass='LC' and documenttype='DOMESTIC'
		and documentsubtype1='CASH' and servicetype='NEGOTIATION'
		and (status='APPROVED' or status='POSTED' or status='POST_APPROVED')
		and (1<=$P{currentMonth} and year(modifieddate)=$P{currentYear}))as dmcashnegotiation,

	-- FOR DOMESTIC STANDBY
	(select count(servicetype) from tradeservice ts
		where documentclass='LC' and documenttype='DOMESTIC'
		and documentsubtype1='STANDBY' and servicetype='OPENING'
		and (status='APPROVED' or status='POSTED' or status='POST_APPROVED')
		and (1<=$P{currentMonth} and year(modifieddate)=$P{currentYear}))as dmstandbyopening,

	(select count(servicetype) from tradeservice ts
		where documentclass='LC' and documenttype='DOMESTIC'
		and documentsubtype1='STANDBY' and servicetype='ADJUSTMENT'
		and (status='APPROVED' or status='POSTED' or status='POST_APPROVED')
		and (1<=$P{currentMonth} and year(modifieddate)=$P{currentYear}))as dmstandbyadjustment,

	(select count(servicetype) from tradeservice ts
		where documentclass='LC' and documenttype='DOMESTIC'
		and documentsubtype1='STANDBY' and servicetype='CANCELLATION'
		and (status='APPROVED' or status='POSTED' or status='POST_APPROVED')
		and (1<=$P{currentMonth} and year(modifieddate)=$P{currentYear}))as dmstandbycancellation,

	(select count(servicetype) from tradeservice ts
		where documentclass='LC' and documenttype='DOMESTIC'
		and documentsubtype1='STANDBY' and servicetype='AMENDMENT'
		and (status='APPROVED' or status='POSTED' or status='POST_APPROVED')
		and (1<=$P{currentMonth} and year(modifieddate)=$P{currentYear}))as dmstandbyamendment,

	(select count(servicetype) from tradeservice ts
		where documentclass='LC' and documenttype='DOMESTIC'
		and documentsubtype1='STANDBY' and servicetype='NEGOTIATION'
		and (status='APPROVED' or status='POSTED' or status='POST_APPROVED')
		and (1<=$P{currentMonth} and year(modifieddate)=$P{currentYear}))as dmstandbynegotiation,

	-- FOR FX NON LC
	(select count(servicetype) from tradeservice ts
		where (documentclass='DA' or documentclass='DP'
		or documentclass='DR' or documentclass='OA')
		and documenttype='FOREIGN'
		and (servicetype='NEGOTIATION'
			or servicetype='NEGOTIATION_ACKNOWLEDGEMENT'
			or servicetype='NEGOTIATION_ACCEPTANCE')
		and (status='APPROVED' or status='POSTED' or status='POST_APPROVED')
		and (1<=$P{currentMonth} and year(modifieddate)=$P{currentYear}))as fxnonlcnegotiation,

	(select count(servicetype) from tradeservice ts
		where (documentclass='DA' or documentclass='DP'
		or documentclass='DR' or documentclass='OA')
		and documenttype='FOREIGN' and servicetype='SETTLEMENT'
		and (status='APPROVED' or status='POSTED' or status='POST_APPROVED')
		and (1<=$P{currentMonth} and year(modifieddate)=$P{currentYear}))as fxnonlcsettlement,

	(select count(servicetype) from tradeservice ts
		where (documentclass='DA' or documentclass='DP'
		or documentclass='DR' or documentclass='OA')
		and documenttype='FOREIGN' and servicetype='CANCELLATION'
		and (status='APPROVED' or status='POSTED' or status='POST_APPROVED')
		and (1<=$P{currentMonth} and year(modifieddate)=$P{currentYear}))as fxnonlccancellation,

	-- FOR DM NON LC
	(select count(servicetype) from tradeservice ts
		where documentclass='DP' and documenttype='DOMESTIC'
		and servicetype='NEGOTIATION'
		and (status='APPROVED' or status='POSTED' or status='POST_APPROVED')
		and (1<=$P{currentMonth} and year(modifieddate)=$P{currentYear}))as dmnonlcnegotiation,

	(select count(servicetype) from tradeservice ts
		where documentclass='DP' and documenttype='DOMESTIC'
		and servicetype='SETTLEMENT'
		and (status='APPROVED' or status='POSTED' or status='POST_APPROVED')
		and (1<=$P{currentMonth} and year(modifieddate)=$P{currentYear}))as dmnonlcsettlement,

	-- FOR IMPORT OTHERS
	(select count(servicetype) from tradeservice ts
		where documentclass='IMPORT_CHARGES' and servicetype='PAYMENT_OTHER'
		and (status='APPROVED' or status='POSTED' or status='POST_APPROVED')
		and (1<=$P{currentMonth} and year(modifieddate)=$P{currentYear}))as paymentotherimportcharges,

	(select count(servicetype) from tradeservice ts
		where documentclass='CDT' and servicetype='PAYMENT'
		and (status='APPROVED' or status='POSTED' or status='POST_APPROVED')
		and (1<=$P{currentMonth} and year(modifieddate)=$P{currentYear}))as cdtcollection,

	(select count(servicetype) from tradeservice ts
		where documentclass='CDT' and servicetype='REMITTANCE'
		and (status='APPROVED' or status='POSTED' or status='POST_APPROVED')
		and (1<=$P{currentMonth} and year(modifieddate)=$P{currentYear}))as cdtremittance,

	(select count(servicetype) from tradeservice ts
		where documentclass='MD' and servicetype='COLLECTION'
		and (status='APPROVED' or status='POSTED' or status='POST_APPROVED')
		and (1<=$P{currentMonth} and year(modifieddate)=$P{currentYear}))as mdcollection,

	(select count(servicetype) from tradeservice ts
		where documentclass='MD' and servicetype='APPLICATION'
		and (status='APPROVED' or status='POSTED' or status='POST_APPROVED')
		and (1<=$P{currentMonth} and year(modifieddate)=$P{currentYear}))as mdapplication,

	(select count(servicetype) from tradeservice ts
		where documentclass='CORRES_CHARGE' and servicetype='SETTLEMENT'
		and (status='APPROVED' or status='POSTED' or status='POST_APPROVED')
		and (1<=$P{currentMonth} and year(modifieddate)=$P{currentYear}))as settlementcorrescharge,

	(select count(servicetype) from tradeservice ts
		where documentclass='REBATE' and servicetype='PROCESS'
		and (status='APPROVED' or status='POSTED' or status='POST_APPROVED')
		and (1<=$P{currentMonth} and year(modifieddate)=$P{currentYear}))as rebates,

	(select count(servicetype) from tradeservice ts
		where documentclass='IMPORT_ADVANCE' and servicetype='PAYMENT'
		and (status='APPROVED' or status='POSTED' or status='POST_APPROVED')
		and (1<=$P{currentMonth} and year(modifieddate)=$P{currentYear}))as collectionimportadvance,

	(select count(servicetype) from tradeservice ts
		where documentclass='IMPORT_ADVANCE' and servicetype='REFUND'
		and (status='APPROVED' or status='POSTED' or status='POST_APPROVED')
		and (1<=$P{currentMonth} and year(modifieddate)=$P{currentYear}))as refundimportadvance,

	(select count(servicetype) from tradeservice ts
		where documentclass='LC' and servicetype='REFUND'
		and (status='APPROVED' or status='POSTED' or status='POST_APPROVED')
		and (1<=$P{currentMonth} and year(modifieddate)=$P{currentYear}))as refundlc,

	-- FOR EXPORT ADVISING
	(select count(servicetype) from tradeservice ts
		where documentclass='EXPORT_ADVISING' and servicetype='OPENING_ADVISING'
		and (status='APPROVED' or status='POSTED' or status='POST_APPROVED')
		and (1<=$P{currentMonth} and year(modifieddate)=$P{currentYear}))as exportadvisingopening,

	(select count(servicetype) from tradeservice ts
		where documentclass='EXPORT_ADVISING' and servicetype='AMENDMENT_ADVISING'
		and (status='APPROVED' or status='POSTED' or status='POST_APPROVED')
		and (1<=$P{currentMonth} and year(modifieddate)=$P{currentYear}))as exportadvisingamendment,

	(select count(servicetype) from tradeservice ts
		where documentclass='EXPORT_ADVISING' and servicetype='CANCELLATION_ADVISING'
		and (status='APPROVED' or status='POSTED' or status='POST_APPROVED')
		and (1<=$P{currentMonth} and year(modifieddate)=$P{currentYear}))
		as exportadvisingcancellation,

	-- FOR EXPORT BILLS
	(select count(servicetype) from tradeservice ts
		where documentclass='BC' and documenttype='FOREIGN'
		and servicetype='NEGOTIATION'
		and (status='APPROVED' or status='POSTED' or status='POST_APPROVED')
		and (1<=$P{currentMonth} and year(modifieddate)=$P{currentYear}))as ebcnegotiation,

	(select count(servicetype) from tradeservice ts
		where documentclass='BP' and documenttype='FOREIGN'
		and servicetype='NEGOTIATION'
		and (status='APPROVED' or status='POSTED' or status='POST_APPROVED')
		and (1<=$P{currentMonth} and year(modifieddate)=$P{currentYear}))as ebpnegotiation,

	(select count(servicetype) from tradeservice ts
		where documentclass='BC' and documenttype='DOMESTIC'
		and servicetype='NEGOTIATION'
		and (status='APPROVED' or status='POSTED' or status='POST_APPROVED')
		and (1<=$P{currentMonth} and year(modifieddate)=$P{currentYear}))as dbcnegotiation,

	(select count(servicetype) from tradeservice ts
		where documentclass='BP' and documenttype='DOMESTIC'
		and servicetype='NEGOTIATION'
		and (status='APPROVED' or status='POSTED' or status='POST_APPROVED')
		and (1<=$P{currentMonth} and year(modifieddate)=$P{currentYear}))as dbpnegotiation,

	(select count(servicetype) from tradeservice ts
		where documentclass='BC' and documenttype='FOREIGN'
		and servicetype='SETTLEMENT'
		and (status='APPROVED' or status='POSTED' or status='POST_APPROVED')
		and (1<=$P{currentMonth} and year(modifieddate)=$P{currentYear}))as ebcsettlement,

	(select count(servicetype) from tradeservice ts
		where documentclass='BP' and documenttype='FOREIGN'
		and servicetype='SETTLEMENT'
		and (status='APPROVED' or status='POSTED' or status='POST_APPROVED')
		and (1<=$P{currentMonth} and year(modifieddate)=$P{currentYear}))as ebpsettlement,

	(select count(servicetype) from tradeservice ts
		where documentclass='BC' and documenttype='DOMESTIC'
		and servicetype='SETTLEMENT'
		and (status='APPROVED' or status='POSTED' or status='POST_APPROVED')
		and (1<=$P{currentMonth} and year(modifieddate)=$P{currentYear}))as dbcsettlement,

	(select count(servicetype) from tradeservice ts
		where documentclass='BP' and documenttype='DOMESTIC'
		and servicetype='SETTLEMENT'
		and (status='APPROVED' or status='POSTED' or status='POST_APPROVED')
		and (1<=$P{currentMonth} and year(modifieddate)=$P{currentYear}))as dbpsettlement,

	-- FOR EXPORT OTHERS
	(select count(servicetype) from tradeservice ts
		where documentclass='EXPORT_ADVANCE' and servicetype='PAYMENT'
		and (status='APPROVED' or status='POSTED' or status='POST_APPROVED')
		and (1<=$P{currentMonth} and year(modifieddate)=$P{currentYear}))as exportadvance,

	(select count(servicetype) from tradeservice ts
		where documentclass='EXPORT_ADVANCE' and servicetype='REFUND'
		and (status='APPROVED' or status='POSTED' or status='POST_APPROVED')
		and (1<=$P{currentMonth} and year(modifieddate)=$P{currentYear}))as refundexportadvance,

	(select count(servicetype) from tradeservice ts
		where documentclass='EXPORT_CHARGES' and servicetype='PAYMENT_OTHER'
		and (status='APPROVED' or status='POSTED' or status='POST_APPROVED')
		and (1<=$P{currentMonth} and year(modifieddate)=$P{currentYear}))as paymentotherexportcharges,

	(select count(servicetype) from tradeservice ts
		where documentclass='EXPORT_CHARGES' and servicetype='REFUND'
		and (status='APPROVED' or status='POSTED' or status='POST_APPROVED')
		and (1<=$P{currentMonth} and year(modifieddate)=$P{currentYear}))as refundotherexportcharges

from tradeservice]]>
	</queryString>
