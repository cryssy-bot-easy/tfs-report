SELECT PAYMENT_REF,
	  IEDIEIRDNO,
	  AABREFCODE,
	  CLIENT_NAME,
	  AMOUNT,
	  REQ_TYPE,
	  DATE(UPLOAD_DATE) AS UPLOAD_DATE,
	  DATE(DATEPAID) AS DATEPAID,
	  STATUS

