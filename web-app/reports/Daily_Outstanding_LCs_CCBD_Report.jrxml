	<queryString>
		<![CDATA[--DAILY OUTSTANDING LCs CCBD
SELECT tp.documentnumber AS documentNumber,
	  tp.CIFNAME,
	  tp.cifnumber AS cifNumber,
	  tp.amount AS originalLcAmount,
	  lc.outstandingbalance AS oslcAmount,
	  lc.expirydate AS lcExpiryDate,
	  Lc.lasttransaction AS lastTransaction,
	  tp.currency AS lcCurrency,
	  tp.STATUS

FROM tradeproduct tp

INNER JOIN letterofcredit lc
ON tp.documentnumber = lc.DOCUMENTNUMBER

WHERE tp.STATUS = 'OPEN'
AND tp.PRODUCTTYPE ='LC'
AND lc.outstandingbalance > 0

ORDER BY lcCurrency]]>
	</queryString>
